<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splash">
    <fragment
        android:id="@+id/splash"
        android:name="com.example.buscacep.Splash"
        android:label="Splash" >
        <action
            android:id="@+id/action_splash_to_home2"
            app:destination="@id/home2" />
    </fragment>
    <fragment
        android:id="@+id/home2"
        android:name="com.example.buscacep.Home"
        android:label="Home" >
        <action
            android:id="@+id/action_home2_to_historic"
            app:destination="@id/historic" />
    </fragment>
    <fragment
        android:id="@+id/historic"
        android:name="com.example.buscacep.Historic"
        android:label="Historic" >
        <action
            android:id="@+id/action_historic_to_home2"
            app:destination="@id/home2" />
    </fragment>
</navigation>